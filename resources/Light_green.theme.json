{
  "name": "Light green",
  "dark": false,
  "author": "caijh",
  "editorScheme": "/Light_green.xml",
  "colors": {
    "bg": "#C7EDCC",
    "blue0": "#167DFF",
    "blue1": "#0000F6",
    "btnStartBg": "#FAF9DE",
    "btnEndBg": "#FAF9DE"
